{
  "name": "todo-ng2-mtl",
  "version": "1.0.0",
  "description": "Yet another todo app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .tmp",
    "clean:dist": "rimraf dist",
    "build": "webpack --config config/webpack-dev.config.js --progress --profile --colors --display-error-details --display-cached",
    "build:dist": "webpack --config config/webpack-prod.config.js --progress --profile --colors --display-error-details --display-cached --bail",
    "launchserver": "webpack-dev-server --config config/webpack-dev.config.js",
    "launchserver:dist": "http-server dist",
    "serve": "npm-run-all clean build launchserver",
    "serve:dist": "npm-run-all clean:dist build:dist launchserver:dist",
    "start": "npm run serve",
    "test": "node --max-old-space-size=4096 node_modules/karma/bin/karma start karma.config.js",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nfang/todo-ng2-mtl.git"
  },
  "keywords": [
    "todo"
  ],
  "author": "Nathan Fang <nathan.fang@icloud.com>",
  "license": "ISC",
  "devDependencies": {
    "awesome-typescript-loader": "^0.16.2",
    "css-loader": "^0.23.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "merge": "^1.2.0",
    "node-sass": "^3.6.0",
    "npm-run-all": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.16",
    "es6-shim": "^0.35.0",
    "lodash": "^4.11.1",
    "normalize.css": "^4.1.1",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "zone.js": "^0.6.10"
  }
}
